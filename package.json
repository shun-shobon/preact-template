{
  "name": "preact-template",
  "version": "0.0.0",
  "description": "preact project template.",
  "homepage": "https://github.com/shun-shobon/preact-template#readme",
  "bugs": "https://github.com/shun-shobon/preact-template/issues",
  "license": "MIT",
  "author": "SHUN/しゅん <shun.shobon@gmail.com> (https://github.com/shun-shobon)",
  "repository": "https://github.com/shun-shobon/preact-template.git",
  "scripts": {
    "build": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' webpack",
    "dev": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' webpack-dev-server",
    "eslint": "eslint --ignore-path .gitignore 'src/**/*.{tsx,ts}'",
    "eslint:fix": "eslint --ignore-path .gitignore 'src/**/*.{tsx,ts}' --fix",
    "stylelint": "stylelint --ignore-path .gitignore 'src/**/*.{scss,css}'",
    "stylelint:fix": "stylelint --ignore-path .gitignore 'src/**/*.{scss,css}' --fix"
  },
  "lint-staged": {
    "**/*.{tsx,ts}": "eslint --fix",
    "**/*.{scss,css}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "preact": "^10.5.4",
    "regenerator-runtime": "^0.13.7",
    "tslib": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/fibers": "^3.1.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/node": "^14.11.5",
    "@types/sass": "^1.16.0",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "fibers": "^5.0.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "postcss": "^8.1.1",
    "postcss-loader": "^4.0.3",
    "prettier": "^2.1.2",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.3.0",
    "stylelint": "^13.7.2",
    "stylelint-config-recess-order": "^2.1.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "private": true
}
